{
  "name": "flexonexus/laravel-tap-payments",
  "description": "Laravel integration for Tap Payments (charges, refunds, webhooks).",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "payments", "tap", "gateway", "ksa", "kuwait"],
  "autoload": {
    "psr-4": {
      "FlexoNexus\\Tap\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": ["FlexoNexus\\Tap\\TapServiceProvider"],
      "aliases": { "Tap": "FlexoNexus\\Tap\\Facades\\Tap" }
    }
  },
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.8|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^8.0",
    "phpunit/phpunit": "^10.5|^11.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
